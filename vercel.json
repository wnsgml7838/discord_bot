{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "bot/discord_bot_problem_recommender.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/cron-logger",
      "schedule": "0 */3 * * *"
    },
    {
      "path": "/api/discord-log-sync",
      "schedule": "*/5 * * * *"
    }
  ]
} 